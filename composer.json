{
    "name": "vyfony/portation-bundle",
    "type": "symfony-bundle",
    "description": "Symfony VyfonyPortationBundle",
    "keywords": ["portation", "import", "export", "symfony-bundle"],
    "homepage": "https://github.com/vyfony/portation-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Dyshkant",
            "email": "vyshkant@gmail.com",
            "homepage": "https://vyshkant.github.io"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "doctrine/orm": "^2.5",
        "phpoffice/phpspreadsheet": "^1.9",
        "symfony/console": "^4.0",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/doctrine-bridge": "^4.0",
        "symfony/http-kernel": "^4.0",
        "symfony/process": "^4.0",
        "symfony/translation": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "php-parallel-lint/php-parallel-lint": "^1.0",
        "phan/phan": "^2.3",
        "phpro/grumphp": "^0.17",
        "phpunit/phpunit": "^8.4",
        "povils/phpmnd": "^2.1",
        "sensiolabs/security-checker": "^4.0",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Vyfony\\Bundle\\PortationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vyfony\\Bundle\\PortationBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
